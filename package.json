{
  "name": "obsidian-keyword-highlighter",
  "version": "1.3.2",
  "description": "Automatically highlight specified keywords within your Obsidian notes for enhanced visibility and quick reference.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.js,.svelte"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.7",
    "@types/node": "25.2.1",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "builtin-modules": "5.0.0",
    "esbuild": "0.27.3",
    "esbuild-svelte": "^0.9.4",
    "eslint-plugin-svelte": "^3.14.0",
    "obsidian": "latest",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte": "^4.2.15",
    "svelte-preprocess": "^6.0.3",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@codemirror/search": "^6.6.0"
  }
}
